.top-td__item{
    display: flex;
    align-items: flex-start;
    line-height: 110rpx;
}
.top-td__item:nth-child(2n){
    background: #f5f5f5;
}
.top-th{
    background: #f5f5f5;
    line-height: 80rpx;

}
.top-td .top-td__lastly{
    color: #999;
}
.top-td__rank{
    font-size: 1.2em;
}
.score-top{
    color: #ff4222;
}
.top-th view {
    color: #999;
}
view{
    text-align: center;
}
.w-15{
    width: 15%;
}
.w-35{
    width: 35%;
}
.w-25{
    width: 25%;
}
.top-td__nickname image{
   width: 90rpx;
   height: 90rpx;
   border-radius: 50%;
   vertical-align: middle;
   margin-right: 10rpx;
}